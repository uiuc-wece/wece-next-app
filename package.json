{
  "name": "weceweb",
  "version": "1.0.0",
  "description": "Code for WECE <a href=\"https://wece.ece.illinois.edu\">website</a>",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "watch": "webpack -d --watch",
    "build": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development",
    "dev": "webpack-dev-server --content-base dist/ --history-api-fallback --open",
    "deploy": "gh-pages master -d dist",
    "predeploy": "npm run build"
  },
